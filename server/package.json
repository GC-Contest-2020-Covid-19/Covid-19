{
    "name": "covid-app",
    "version": "0.1.0",
    "private": true,
    "main": "server.js",
    "engines": {
        "node": "12.x"
    },
    "scripts": {
        "client-install": "cd ../client && npm install",
        "start": "node server.js",
        "server": "nodemon server.js",
        "client": "npm start --prefix ../client",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "devDependencies": {
        "nodemon": "^1.14.6"
    },
    "dependencies": {
        "concurrently": "^3.5.1",
        "dotenv": "^8.2.0",
        "express": "^4.16.2",
        "mongoose": "^5.9.14",
        "node-fetch": "^2.6.1",
        "node-geocoder": "^3.27.0",
        "puppeteer": "^3.0.1"
    }
}
